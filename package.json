{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/main",
    "start:nest": "nest start",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "test:load": "./node_modules/artillery/bin/artillery run artillery.load.yml",
    "artillery:cli": "./node_modules/artillery/bin/artillery",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "typeorm:migration": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts migration:run",
    "typeorm:generate": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts migration:generate -n newTables -d migrations",
    "heroku-postbuild": "nest build && npm run typeorm:migration"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "close-with-grace": "^1.1.0",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "fastify-file-interceptor": "^1.0.8",
    "fastify-helmet": "^7.1.0",
    "fastify-multer": "^2.0.3",
    "fastify-swagger": "^5.2.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "pino": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.17",
    "@types/pino": "^7.0.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "artillery": "^2.0.0-27",
    "chai": "4.3.7",
    "debug": "4.3.4",
    "dirty-chai": "2.0.1",
    "eslint": "8.30.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.1",
    "supertest": "6.3.3",
    "typescript": "^4.9.4",
    "yamljs": "0.3.0"
  }
}
